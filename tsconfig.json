{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["esnext", "dom"],
    "module": "commonjs",
    "types": ["cypress", "node"],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true
  },
  "include": [
    "cypress/**/*.ts",
    "cypress.config.ts",
    "cypress/support/*.ts",
    "cypress/utils/*.ts"
  ]
}
